{
    "manifest_version": 3,
    "name": "LinkedIn Scraper",
    "version": "1.0",
    "description": "Scrapes LinkedIn profiles and sends the data to an API.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "scripting"
    ],

    "host_permissions": [
      "https://*.linkedin.com/*",
      "http://localhost/*"
    ]
  }
  